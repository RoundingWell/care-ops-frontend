  "nyc": {
    "temp-directory": "./coverage",
    "report-dir": "./coverage",
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/js/base/**/*.js",
      "src/js/auth.js",
      "src/js/index.js"
    ]
  },
